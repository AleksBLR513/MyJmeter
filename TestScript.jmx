            <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Создание CSV для импорта" enabled="true">
              <stringProp name="scriptLanguage">groovy</stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">import java.util.UUID
import java.util.Random

// Создаем экземпляр Random для генерации случайных чисел
def random = new Random()

// Данные для записи
def data = [
    [&quot;Тестовое поле&quot;, &quot;Имя&quot;, &quot;Фамилия&quot;, &quot;Пол&quot;, &quot;Мобильный телефон&quot;, &quot;Почта (идентификатор)&quot;, &quot;Отдел&quot;, &quot;Дата увольнения&quot;]
]

// Генерация 2 строк с данными
for (int i = 0; i &lt; 2; i++) {
    def row = [
        random.nextInt(100000), // Случайное число от 0 до 99999 Тестовое поле
        UUID.randomUUID().toString().substring(0, 5), // Уникальное Имя
        UUID.randomUUID().toString().substring(0, 5), // Уникальная Фамилия
        1, // Пол
        &apos; &apos; + &quot;+&quot; + (1..11).collect { random.nextInt(10) }.join(&apos;&apos;), // Случайный мобильный телефо
        UUID.randomUUID().toString().substring(0, 5) + &quot;@mail.ru&quot;, // Уникальная Почта
        &quot;&quot;, // Отдел (пустое значение)
        &quot;&quot;  // Дата увольнения (пустое значение)
    ]
    data.add(row) // Добавляем сгенерированную строку в список
}

// Определяем путь к файлу CSV
def filePath = &quot;import.csv&quot;

// Создаём файловый поток с кодировкой UTF-8
def writer = new OutputStreamWriter(new FileOutputStream(filePath), &quot;UTF-8&quot;)

// Записываем заголовки в файл
writer.write(data[0].join(&apos;;&apos;) + &apos;\n&apos;) // Используем ; как разделитель

// Записываем остальные строки
for (int i = 1; i &lt; data.size(); i++) {
    writer.write(data[i].join(&apos;;&apos;) + &apos;\n&apos;) // Используем ; как разделитель
}

writer.close()

log.info(&quot;CSV файл успешно создан: &quot; + filePath)</stringProp>
